                <version>${blog.version}</version>
            </dependency>

            <dependency>
                <groupId>com.mugu.blog</groupId>
                <artifactId>blog-rabbitmq-starter</artifactId>
                <version>${blog.version}</version>
            </dependency>

            <dependency>
                <groupId>com.mugu.blog</groupId>
                <artifactId>blog-skywalking-starter</artifactId>
                <version>${blog.version}</version>
            </dependency>

            <dependency>
                <groupId>com.mugu.blog</groupId>
