            <dependency>
                <groupId>com.mugu.blog</groupId>
                <artifactId>article-common</artifactId>
                <version>${blog.version}</version>
            </dependency>

            <dependency>
                <groupId>com.mugu.blog</groupId>
                <artifactId>article-api</artifactId>
                <version>${blog.version}</version>
            </dependency>

            <dependency>
                <groupId>com.mugu.blog</groupId>
                <artifactId>blog-nacos-starter</artifactId>
                <version>${blog.version}</version>
            </dependency>

            <dependency>
                <groupId>com.mugu.blog</groupId>
